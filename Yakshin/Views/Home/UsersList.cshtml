@model IEnumerable<Yakshin.Models.Users>

<h2>Список пользователей</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Имя</th>
            <th>Фамилия</th>
            <th>Имя пользователя</th>
            <th>Действия</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.ID</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Username</td>
                <td>
                    <a asp-action="EditUser " asp-route-id="@user.ID">Редактировать</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="AdminPage">Назад к администраторской странице</a>
